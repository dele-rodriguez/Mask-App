<p> <%= username %>  you can view your sent messages here</p>

<div>
    <% if(userMessages) { %>
        <% userMessages.map((message , index) => { %>
            <div>
                <p><%= message.maskMessage %></p>
                <p><%= message.sentAt %> </p>
            </div>
        <% }) %>
    <% } else { %>
        <div>
